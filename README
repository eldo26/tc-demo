docker run --name mysql-server -v /data/mysql/data:/var/lib/mysql --restart=always -p 3306:3306 -e MYSQL_ROOT_PASSWORD=root -d mysql 

mysql> create database tc;
mysql> create user 'tc2'@'172.17.0.1' identified by 'tc2';
mysql> grant all on tc.* to 'tc2'@'172.17.0.1';



